@model IEnumerable<TSSPruebaInventario.Models.Ubicacion>

<h4>Ubicaciones del Producto Selecionado</h4>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
    </tr>
}

</table>
